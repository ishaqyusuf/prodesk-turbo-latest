model HousePackageTools {
    id             Int                @unique @default(autoincrement())
    orderItemId    Int                @unique
    priceId        String?            @unique
    height         String?
    doorType       String?
    doorId         Int?
    dykeDoorId     Int?
    jambSizeId     Int?
    casingId       Int?
    moldingId      Int?
    stepProductId  Int?
    totalPrice     Float?             @default(0)
    totalDoors     Int?               @default(0)
