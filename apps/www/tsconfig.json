{
    "extends": "@gnd/tsconfig/midday-nextjs.json",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["./*"]
        },
        "strict": true
    },
    "include": [
        "next-env.d.ts",
        "next.config.js",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts",
        "tailwind.config.ts"
    ],
    "exclude": ["node_modules"]
}
